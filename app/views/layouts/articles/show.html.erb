<!DOCTYPE html>
<html lang="en">
    <head>
      <title>Hlm</title>
      <meta charset="utf-8">
      <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
      <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
      <%= csrf_meta_tags %>
    </head>
    <body>

        <%= render 'layouts/header' %>

        <div class="container">
          <hr>
          <div class="row">
            <div class="col-lg-3">
              <%= render 'layouts/sidebar_actions' %>

              <%= render 'layouts/articles/sidebar_show' %>
            </div>
            <div class="col-lg-9">
              <%= render 'layouts/messages' %>
              <%= yield %>
            </div>
          </div>
        </div>

    </body>
</html>
